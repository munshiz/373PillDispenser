#include "Pills.h"

pillEntry pillList [50];

void pillsinit(){
	numPills = 0;
}
void addNewEntry(uint8_t dispenserNum, char * name, uint8_t dayofWeek, uint8_t hour, uint8_t min){
	pillEntry new = {numPills, dispenserNum, name, daysofWeek, hour, min, 1};
	pillList[numPills + 1] = new;
	sortEntries();
}

